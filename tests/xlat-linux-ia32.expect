@rootpgt: PGT
  target_as=MACHPHYSADDR
  root=MACHPHYSADDR:0x911000
  pte_mask=0x0
  pte_format=ia32
  fields=12,10,10

@direct: LINEAR
  target_as=KPHYSADDR
  off=0xffffffff40000000

@rdirect: LINEAR
  target_as=KVADDR
  off=0xc0000000

@machphys_kphys: LINEAR
  target_as=KPHYSADDR
  off=0x0

@kphys_machphys: LINEAR
  target_as=MACHPHYSADDR
  off=0x0

KV -> HW:
0-ffffffff: @rootpgt
100000000-ffffffffffffffff: NONE

KV -> PHYS:
0-ffffffff: @rootpgt
100000000-ffffffffffffffff: NONE

KPHYS -> DIRECT:
0-3fffffff: @rdirect
40000000-ffffffffffffffff: NONE

MACHPHYS -> KPHYS:
0-ffffffff: @machphys_kphys
100000000-ffffffffffffffff: NONE

KPHYS -> MACHPHYS:
0-ffffffff: @kphys_machphys
100000000-ffffffffffffffff: NONE
