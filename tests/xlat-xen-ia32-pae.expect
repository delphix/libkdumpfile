@rootpgt: PGT
  target_as=MACHPHYSADDR
  root=MACHPHYSADDR:0x100120
  pte_mask=0x0
  pte_format=ia32_pae
  fields=12,9,9,2

@machphys_kphys: LINEAR
  target_as=KPHYSADDR
  off=0x0

@kphys_machphys: LINEAR
  target_as=MACHPHYSADDR
  off=0x0

KV -> HW:
0-ffffffff: @rootpgt
100000000-ffffffffffffffff: NONE

KV -> PHYS:
0-ffffffff: @rootpgt
100000000-ffffffffffffffff: NONE

KPHYS -> DIRECT:

MACHPHYS -> KPHYS:
0-fffffffffffff: @machphys_kphys
10000000000000-ffffffffffffffff: NONE

KPHYS -> MACHPHYS:
0-fffffffffffff: @kphys_machphys
10000000000000-ffffffffffffffff: NONE
