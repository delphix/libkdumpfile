@rootpgt: PGT
  target_as=MACHPHYSADDR
  root=NOADDR
  pte_mask=0x0
  pte_format=ia32
  fields=12,10,10

@machphys_kphys: LINEAR
  target_as=KPHYSADDR
  off=0x0

@kphys_machphys: LINEAR
  target_as=MACHPHYSADDR
  off=0x0

KV -> HW:
0-ffffffff: @rootpgt
100000000-ffffffffffffffff: NONE

KV -> PHYS:
0-ffffffff: @rootpgt
100000000-ffffffffffffffff: NONE

KPHYS -> DIRECT:

MACHPHYS -> KPHYS:
0-ffffffff: @machphys_kphys
100000000-ffffffffffffffff: NONE

KPHYS -> MACHPHYS:
0-ffffffff: @kphys_machphys
100000000-ffffffffffffffff: NONE
